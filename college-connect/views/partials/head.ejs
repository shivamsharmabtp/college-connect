  function loaded() {
    $(".loader").fadeOut(200, function () {
      $(".loading").fadeOut(400);
    });

    $.ajaxSetup({
      headers: { "x-auth": token },
    });
  }
  // TODO : Implement header icon logic here (like Menu)
  token = localStorage.getItem("x-auth");
</script>
